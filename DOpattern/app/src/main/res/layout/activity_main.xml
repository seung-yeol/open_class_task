<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.doteam.dopattern.MainActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="볼륨UP 앱연동"
        android:id="@+id/upsetbutton"
        android:layout_below="@+id/textView3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="96dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="볼륨 DOWN 앱 연동"
        android:id="@+id/downsetbutton"
        android:layout_centerVertical="true"
        android:layout_below="@+id/upsetbutton"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="볼륨 UP연동 삭제"
        android:id="@+id/updeletebutton"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/downsetbutton"/>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="볼륨DOWN연동 삭제"
        android:id="@+id/downdeletebutton"

        android:layout_below="@+id/updeletebutton"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/startname"
        android:id="@+id/textView3"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="45dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lock 시작"
        android:id="@+id/onbtn"
        android:layout_marginTop="65dp"
        android:layout_below="@+id/updeletebutton"
        android:layout_toLeftOf="@+id/updeletebutton"
        android:layout_toStartOf="@+id/updeletebutton" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lock 정지"
        android:id="@+id/offbtn"
        android:layout_alignTop="@+id/onbtn"
        android:layout_toRightOf="@+id/updeletebutton"
        android:layout_toEndOf="@+id/updeletebutton" />




</RelativeLayout>
